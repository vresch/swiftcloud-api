// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Song {
  id    Int     @default(autoincrement()) @id
  song String  @unique
  artist  String
  writer String
  album String
  year Int
  playsJune Int
  playsJuly Int
  playsAugust Int
}
